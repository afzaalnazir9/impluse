{
  "type": "header",
  "name": "Header group",
  "sections": {
    "announcement-bar": {
      "type": "announcement-bar",
      "blocks": {
        "message": {
          "type": "message",
          "settings": {
            "text": "Free Shipping On All Orders Over $50",
            "url": ""
          }
        }
      },
      "block_order": [
        "message"
      ],
      "settings": {
        "enable_sticky": true,
        "autoplay": false,
        "cycle_speed": 4,
        "text_size": "text-base",
        "navigation_mode": "arrows",
        "scrolling_speed": 30,
        "background": "#1a1a1a",
        "background_gradient": "",
        "text_color": "#ffffff"
      }
    },
    "header": {
      "type": "header",
      "settings": {
        "enable_sticky": true,
        "hide_on_scroll": false,
        "logo": "{{shop.brand.logo}}",
        "logo_max_width": 120,
        "logo_mobile_max_width": 100,
        "menu": "main-menu",
        "sidebar_menu": "main-menu",
        "secondary_menu": "",
        "menu_open_trigger": "click",
        "opening_from": "bottom",
        "layout": "logo_center_navigation_inline",
        "show_social_media": true,
        "show_locale_selector": false,
        "show_country_selector": false,
        "show_country_flag": false,
        "show_country_name": false,
        "transparent_header_text_color": "#000000",
        "reduce_padding": false,
        "background_opacity": 100,
        "background_blur_radius": 0
      }
    }
  },
  "order": [
    "announcement-bar",
    "header"
  ]
}
